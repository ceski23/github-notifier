{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "GitHub Notifier",
  "version": "0.0.1",
  "identifier": "pl.ceski23.github-notifier",
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["github-notifier"]
      },
      "mobile": []
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": []
  }
}